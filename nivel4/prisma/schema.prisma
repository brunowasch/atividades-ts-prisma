datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Tarefa {
  id         Int      @id @default(autoincrement())
  titulo     String
  concluida  Boolean  @default(false)
  deletadoEm DateTime?
  createdAt  DateTime @default(now())
}